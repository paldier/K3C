#!/bin/sh
# Configure AccessPoint.X_LANTIQ_COM_Vendor.HS20 parameters
# Needed object: AccessPoint.X_LANTIQ_COM_Vendor.HS20

script_name="$0"

[ ! "$LIB_COMMON_SOURCED" ] && . /tmp/fapi_wlan_wave_lib_common.sh
[ ! "$LIB_CONVERT_SOURCED" ] && . /tmp/fapi_wlan_wave_lib_convert.sh
[ ! "$IN_CONF_SOURCED" ] && in_conf_source

print2log $radio_index $debug_level "script_name=$script_name"

interface_name=$1
pid=$$
ubus_call=${CONF_DIR}/ubus_call.sh
hs20_conf_old_not_exist=0

# Find the interface index and the radio index
interface_index=`find_index_from_interface_name $interface_name`
radio_name=${interface_name%%.*}
radio_index=`find_index_from_interface_name $radio_name`

debug_level=DEBUG
#debug_level=HS20

print2log $radio_index $debug_level "$script_name $*"
timestamp $radio_index  "$script_name:begin"

print2log $radio_index $debug_level "fapi_wlan_wave_hotspot_set: interface_index=$interface_index, radio_index=$radio_index"

if [ -e ${HS20_CONF} ]
then
	#if VAP is not in fapi_wlan_wave_hs20_conf and both Enable and OSEN are disable - do nothing
	enable=`cat ${HS20_CONF} | grep ^Enable_${interface_index}`
	osen=`cat ${HS20_CONF} | grep ^OSENEnabled_${interface_index}`
	print2log $radio_index $debug_level "fapi_wlan_wave_hotspot_set: enable=$enable, osen=$osen"
	if [ -z "$enable" ] && [ -z "$osen" ]
	then
		# First time VAP is called for HS20
		print2log $radio_index $debug_level "fapi_wlan_wave_hotspot_set: First time VAP is called for HS20"
		enable=`cat /tmp/wlan_wave/hotspot_set_in_conf | grep ^Enable_0`
		osen=`cat /tmp/wlan_wave/hotspot_set_in_conf | grep ^OSENEnabled_0`
		print2log $radio_index $debug_level "fapi_wlan_wave_hotspot_set: IN_CONF: enable=$enable, osen=$osen"
		# Read from IN_CONF, if non of the parameters is true - return
		enable=${enable##*=}
		enable=$(printf "%b" "$enable")
		osen=${osen##*=}
		osen=$(printf "%b" "$osen")
		print2log $radio_index $debug_level "fapi_wlan_wave_hotspot_set: IN_CONF 2: enable=$enable, osen=$osen"
		if [ "$enable" = \"false\" ] && [ "$osen" = \"false\" ]
		then
			print2log $radio_index $debug_level "fapi_wlan_wave_hotspot_set: VAP is not HS20, return"
			update_conf_out "wlan_configuration_status" "success"
			return
		fi	
	fi
else
	print2log $radio_index $debug_level "fapi_wlan_wave_hotspot_set: ${HS20_CONF} not exist (init)"
	enable=`cat ${IN_CONF} | grep ^Enable_`
	osen=`cat ${IN_CONF} | grep ^OSENEnabled_`
	print2log $radio_index $debug_level "fapi_wlan_wave_hotspot_set: enable=$enable, osen=$osen"
	# First time VAP is called for HS20
	# Read from IN_CONF, if non of the parameters is true - return
	enable=${enable##*=}
	enable=$(printf "%b" "$enable")
	osen=${osen##*=}
	osen=$(printf "%b" "$osen")
	print2log $radio_index $debug_level "fapi_wlan_wave_hotspot_set: IN_CONF 2: enable=$enable, osen=$osen"
	if [ "$enable" = \"false\" ] && [ "$osen" = \"false\" ]
	then
		print2log $radio_index $debug_level "fapi_wlan_wave_hotspot_set: interface is not HS20, return"
		update_conf_out "wlan_configuration_status" "success"
		return
	fi	
fi

# mv temporary conf files.
# Save last configuration before update with new one.
# Use to compare, detect changes.
#cp ${HS20_CONF}  ${CONF_DIR}/_fapi_wlan_wave_hs20_conf
#rm ${CONF_DIR}/fapi_wlan_wave_hs20_*
#mv ${CONF_DIR}/_fapi_wlan_wave_hs20_conf ${HS20_CONF}
