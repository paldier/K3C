#[ -e ${HS20_COMMANDS_SAVE}_$radio_name ] && rm ${HS20_COMMANDS_SAVE}_$radio_name
[ -e ${HS20_COMMANDS_SAVE}_$radio_name ] && sed -i '/'${interface_name}'$/d' ${HS20_COMMANDS_SAVE}_$radio_name
[ -e ${HS20_COMMANDS_SAVE}_$radio_name ] && sed -i '/'${interface_name}' /d' ${HS20_COMMANDS_SAVE}_$radio_name
