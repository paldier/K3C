+/*
+ *	form 64bit register from 2 32bit DWORDs, 64-bit register is little endian mode
+ */
+uint64_t cbm_form_64bit_reg(uint32_t dw0, uint32_t dw1)
+{
+	uint64_t reg64;
+	reg64 = (((uint64_t)dw1) << 32) | (uint64_t)dw0;
+	return reg64;
