+uint32_t flags)
+{
+	uint32_t op_type = 0;
+	int j;
+	struct cbm_qidt_elm qidt_set = {0};
+	struct cbm_qidt_mask qidt_mask = {1, 1, 1, 1, 1, 1, 1, 1};
