+static struct ltq_lro_port *ltq_allocate_lro_port (void)
+{
+	int i;
+
+	for (i = 0; i < LTQ_MAX_LRO_PORTS; i++) {
+		if (!ltq_lro_port[i].in_use) {
+			ltq_lro_port[i].in_use = 1;
+			return &ltq_lro_port[i];
+		}
+	}
+	return NULL;
+}
