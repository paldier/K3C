+static struct ltq_lro_port *ltq_lookup_lro_port(int session_id)
+{
+	int i;
+
+	for (i = 0; i < LTQ_MAX_LRO_PORTS; i++) {
+		if (ltq_lro_port[i].session_id == session_id) {
+			return &ltq_lro_port[i];
+		}
+	}
+	return NULL;
