+static inline uint8_t
+_dc_dp_get_class2wmm(int32_t dev_idx, uint8_t class, bool is_class)
+{
+	uint8_t wmm;
+	uint8_t prio;
+
+	if (is_class) {
+		class = (class & 0x0F);
+		prio = g_priv_info[dev_idx].class2prio[class];
+	} else {
+		prio = class;
+	}
+
+	prio = (prio & DC_WIFI_CLASS_MASK);
+	wmm = g_priv_info[dev_idx].prio2wmm[prio];
+
+	return wmm;
+}
