+		hlist_for_each_entry_rcu(sta_entry, &g_sta_info.sta_hash[i], hlist) {
+			seq_printf(seq, "%pM    %d        %s\n",
+				sta_entry->mac_addr, sta_entry->sta_id,
+				((sta_entry->netif) ? sta_entry->netif->name : NULL));
