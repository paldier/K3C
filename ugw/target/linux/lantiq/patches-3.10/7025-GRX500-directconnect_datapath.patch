+static __init int dc_dp_init_module(void)
+{
+	int32_t ret = 0;
+
+	spin_lock_init(&g_dc_dp_lock);
+
+	DC_DP_LOCK(&g_dc_dp_lock);
+
+	if (!g_dc_dp_init_ok) {
+		memset(g_priv_info, 0, sizeof(g_priv_info));
