+   /* set PBLx8 */
+   DWC_REG_WR_BIT(DMA_CR(qInx), DMA_CR_PBLx8, 0x1);
+   /* set TX PBL = 256 */
+   DWC_REG_WR_FIELD(DMA_TCR(qInx), DMA_TCR_PBL, 32);
