+   if (rx_desc_data->use_riwt) {
+      DWC_REG_WR_FIELD(DMA_RIWTR(qInx), DMA_RIWTR_RWT, rx_desc_data->rx_riwt);
+   } else {
+      DWC_REG_WR_FIELD(DMA_RIWTR(qInx), DMA_RIWTR_RWT, 0);
