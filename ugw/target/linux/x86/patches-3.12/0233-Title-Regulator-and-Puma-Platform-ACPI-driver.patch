+static struct attribute_group *puma_acpi_sysfs_entries[] = {
+   &dev_attr_system_state.attr,
+   &dev_attr_netip_state.attr,
+   &dev_attr_pmc_state.attr,
+   &dev_attr_platform_resource.attr,
+   NULL,
+};
