+   PUMA_ACPI_RESOURCE_OFF,      
+};
+
+enum puma_pcie_port_state {
+   PUMA_PCIE_PORT_ENABLE,
+   PUMA_PCIE_PORT_DISABLE,         
+};
+
+enum puma_acpi_netip_request_reset {
+   PUMA_ACPI_NETIP_REQUEST_RESET_WARM = 1,
+   PUMA_ACPI_NETIP_REQUEST_RESET_COLD = 2,      
