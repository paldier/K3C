+static int DWC_ETH_QOS_FRAME_PATTERN_CH[8] = {
+	0x11111111,
+	0x22222222,
+	0x33333333,
+	0x44444444,
+	0x55555555,
+	0x66666666,
+	0x77777777,
+	0x88888888,
