+void DWC_ETH_QOS_free_pg(struct DWC_ETH_QOS_prv_data *pdata)
+{
+	DBGPR_PG("-->DWC_ETH_QOS_free_pg\n");
+
+	//kfree(pdata->pg->pg_ch_input);
+	kfree(pdata->pg);
+
+	DBGPR_PG("<--DWC_ETH_QOS_free_pg\n");
+}
