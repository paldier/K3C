+	}
+
+	pdata->l3_l4_filter = !!flags;
+	hw_if->config_l3_l4_filter_enable(pdata->l3_l4_filter);
+
+	DBGPR_FILTER("Succesfully %s L3/L4 filtering\n",
+		(flags ? "ENABLED" : "DISABLED"));
