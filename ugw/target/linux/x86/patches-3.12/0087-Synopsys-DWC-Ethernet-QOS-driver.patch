+static int check_for_valid_ipv4_addr(char *ip)
+{
+	int i = 0, ret = 0;
+	char *token = NULL;
+
+	token = strtok(ip, ".");
+	if (token) {
+		while(1) {
+			token = strtok(NULL, ".");
+			if (!token) {
+				break;
+			}
+			i++;
+		}
+	}
+
+	if (i != 3)
+		ret = -1;
+
+	return ret;
+}
+
+static int check_for_valid_ipv6_addr(char *ip)
+{
+	int i = 0, ret = 0;
+	char *token = NULL;
+
+	token = strtok(ip, ":");
+	if (token) {
+		while(1) {
+			token = strtok(NULL, ":");
+			if (!token) {
+				break;
+			}
+			i++;
+		}
+	}
+
+	if (i != 7)
+		ret = -1;
+
+	return ret;
+}
