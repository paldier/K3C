+int populate_filter_frames(struct ifr_data_struct *data)
+{
+	int i;
+
+	for (i = 0; i < DWC_ETH_QOS_RWK_FILTER_LENGTH; i++)
+		data->rwk_filter_values[i] = wakeup_filter_config[i];
+
+	data->rwk_filter_length = DWC_ETH_QOS_RWK_FILTER_LENGTH;
+
+	return 0;
+}
