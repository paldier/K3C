+			tmp = strdup(ip);
+			ret = check_for_valid_ipv4_addr(tmp);
+			if (!ret) {
+			  l3_filter.ip4_addr[0] = strtol(strtok(ip, "."), NULL, 10);
+			  l3_filter.ip4_addr[1] = strtol(strtok(NULL, "."), NULL, 10);
