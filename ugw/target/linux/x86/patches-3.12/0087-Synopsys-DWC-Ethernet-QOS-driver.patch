+
+static UINT rx_tstamp_available(t_RX_NORMAL_DESC *rxdesc)
+{
+  UINT varRS1V;
+  UINT varTSA;
+
+  RX_NORMAL_DESC_RDES3_RS1V_Mlf_Rd(rxdesc->RDES3, varRS1V);
+  if (varRS1V == 1) {
+    RX_NORMAL_DESC_RDES1_TSA_Mlf_Rd(rxdesc->RDES1, varTSA);
+    return varTSA;
+  }
+  else {
+    return 0;
+  }
+}
