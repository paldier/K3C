+
+	DBGPR_FILTER("-->DWC_ETH_QOS_config_l3_l4_filtering\n");
+
+	if (flags && pdata->l3_l4_filter) {
+		printk(KERN_ALERT
+			"L3/L4 filtering is already enabled\n");
