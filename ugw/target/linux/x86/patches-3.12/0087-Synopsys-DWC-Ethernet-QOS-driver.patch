+
+	if (display_avb_algo) {
+		fprintf(fp, "Ch AVB Algorithm\n");
+		for (i = 0; i < tx_queue_count; i++) {
+			if (pg_ch_input[i].ch_operating_mode == eDWC_ETH_QOS_QAVB)
+				fprintf(fp, "%s[CH%d]  %s\n", space, i,
