+
+static void DWC_ETH_QOS_free_queue_struct(struct DWC_ETH_QOS_prv_data *pdata)
+{
+	DBGPR("-->DWC_ETH_QOS_free_queue_struct\n");
+
+	if (pdata->tx_queue != NULL) {
+		kfree(pdata->tx_queue);
+		pdata->tx_queue = NULL;
+	}
+
+	if (pdata->rx_queue != NULL) {
+		kfree(pdata->rx_queue);
+		pdata->rx_queue = NULL;
+	}
+
+	DBGPR("<--DWC_ETH_QOS_free_queue_struct\n");
