+			ch_bw = (float)((ch_bw/time)/(1024*1024));
+		fprintf(fp, "%s[CH%d]  %3.2F Mbps\n",
+			space, i, ch_bw);
+		total_ch_bw += ch_bw;
+	}
+	fprintf(fp, "%s----------------\n", space);
+	fprintf(fp, "%sTotal %3.2F Mbps\n", space, total_ch_bw);
+
+	fprintf(fp, "===================================================================\n");
+	fprintf(fp, "\n");
+
+	if (fp == stdout) {
+		printf("Enter 99 to return to main menu::");
+		do {
