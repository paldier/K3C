++/**
++ * Disable external interrupt.
++ *
++ * \param   exint - Number of external interrupt (starts from SVIP_EIU_START)
++ * \return  0 on success
++ * \ingroup API
++ */
++int svip_disable_external_int(u32 exint)
++{
++	int port;
