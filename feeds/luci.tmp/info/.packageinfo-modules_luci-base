Config:
   config LUCI_SRCDIET
	bool "Minify Lua sources"
	default n

   menu "Translations"

     config LUCI_LANG_ca
	   tristate "Catalan (ca)" 

     config LUCI_LANG_cs
	   tristate "Czech (cs)" 

     config LUCI_LANG_de
	   tristate "German (de)" 

     config LUCI_LANG_el
	   tristate "Greek (el)" 

     config LUCI_LANG_en
	   tristate "English (en)" 

     config LUCI_LANG_es
	   tristate "Spanish (es)" 

     config LUCI_LANG_fr
	   tristate "French (fr)" 

     config LUCI_LANG_he
	   tristate "Hebrew (he)" 

     config LUCI_LANG_hu
	   tristate "Hungarian (hu)" 

     config LUCI_LANG_it
	   tristate "Italian (it)" 

     config LUCI_LANG_ja
	   tristate "Japanese (ja)" 

     config LUCI_LANG_ms
	   tristate "Malay (ms)" 

     config LUCI_LANG_no
	   tristate "Norwegian (no)" 

     config LUCI_LANG_pl
	   tristate "Polish (pl)" 

     config LUCI_LANG_pt
	   tristate "Portuguese (pt)" 

     config LUCI_LANG_pt-br
	   tristate "Brazialian Portuguese (pt-br)" 

     config LUCI_LANG_ro
	   tristate "Romanian (ro)" 

     config LUCI_LANG_ru
	   tristate "Russian (ru)" 

     config LUCI_LANG_sk
	   tristate "Slovak (sk)" 

     config LUCI_LANG_sv
	   tristate "Swedish (sv)" 

     config LUCI_LANG_tr
	   tristate "Turkish (tr)" 

     config LUCI_LANG_uk
	   tristate "Ukrainian (uk)" 

     config LUCI_LANG_vi
	   tristate "Vietnamese (vi)" 

     config LUCI_LANG_zh-cn
	   tristate "Chinese (zh-cn)" 

     config LUCI_LANG_zh-tw
	   tristate "Taiwanese (zh-tw)"

   endmenu
